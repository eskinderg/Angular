    "@ngrx/effects": "^8.0.0",
    "@ngrx/store-devtools": "^8.0.0",
    "@ngx-translate/core": "^11.0.0",
    "@types/gapi.auth2": "^0.0.55",
    "angular-oauth2-oidc": "^8.0.0",
    "bootstrap": "^4.1.3",
    "browser-sync": "^2.26.3",
    "core-js": "3.1.3",
