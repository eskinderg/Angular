// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "dist": true,
        "documentation": true,
        "oidc": true,
        "release": true,
        "server": true,
        "coverage": true
    },
    "sqltools.connections": [
        {
            "askForPassword": true,
            "database": "mydb",
            "driver": "MySQL",
            "mysqlOptions": {
                "authProtocol": "default"
            },
            "name": "MyConnection",
            "port": 3306,
            "previewLimit": 50,
            "server": "localhost",
            "username": "esk"
        }
    ]
}
